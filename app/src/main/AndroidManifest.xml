<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.reastaurantapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TotalsActivity" android:label="@string/app_name" > </activity>
        <activity android:name=".FoodsActivity" android:label="@string/app_name" > </activity>
        <activity android:name=".DrinksActivity" android:label="@string/app_name"> </activity>
        <activity android:name=".AddFoods" android:label="@string/app_name"> </activity>
        <activity android:name=".AddDrinks" android:label="@string/app_name"></activity>
    </application>

</manifest>